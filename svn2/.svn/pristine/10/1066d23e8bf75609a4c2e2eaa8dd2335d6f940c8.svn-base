    <script type="text/javascript">
        var i = true;
        var safetyResult = "159357";
        function login() {
            //获取用户输入的用户名和密码
            var tel = document.getElementsByName("tel")[0].value;
            var pwd = document.getElementsByName("pwd")[0].value;
            //ajax
            //参数1：是controller的url
            //参数2：请求的参数（前端传入controller的参数）
            //参数3：后台（controller）返回结果接收的方法
            $.post("login",{tel:tel,pwd:pwd},function (data) {
                if(data.code==0){
                   $("#form").submit();
                } else {
                    alert(data.msg);
                }
            });
        }
        function dx() {
            if(i){
                var phone = document.getElementById("telphone").value;
                if(phone == "" || phone.length<10){
                    alert("电话号码为空或者格式不对")
                }else {
                    $.post("judgephone1",{phone:phone},function (data) {
                        if(data == "000"){
                            alert("当前账号没有注册，无法登录，可以先注册在使用此方法登录");
                        }else {
                            i = false;
                            $.post("dxyz",{phone:phone},function (data) {
                                safetyResult = data;
                            });
                            document.getElementById("yzm1").style.backgroundColor = "Gray";
                            document.getElementById("yzm1").innerHTML = 60+"s";
                            var j=parseInt(document.getElementById("yzm1").innerHTML);
                            var t1=setInterval(function() {
                                if (j > 1) {
                                    j--;
                                    document.getElementById("yzm1").innerHTML=j+"s";}
                                else {
                                    clearInterval(t1);
                                    document.getElementById("yzm1").style.backgroundColor = "#b01f2e";
                                    document.getElementById("yzm1").innerHTML="发送验证码";
                                    i = true;
                                }
                            }, 1000);
                        }
                    });
                }
            }
        }
        function dl() {
            var yam = document.getElementById("yzm").value;
            if(safetyResult == yam){
                var phone = document.getElementById("telphone").value;
                $.post("judgephone1",{phone:phone},function (data) {
                    var pwd = data.pwd;
                    var tel = data.tel;
                    $.post("login",{tel:tel,pwd:pwd},function (data) {
                        if(data.code==0){
                            $("#form1").submit();
                        }
                    });
                });
            }else if(yam!="" && safetyResult != yam){
                alert("验证码不正确");
            }else if(yam == "" || yam.length<5){
                alert("验证码为空或者格式不对，正确格式为6位数字")
            }
        }
