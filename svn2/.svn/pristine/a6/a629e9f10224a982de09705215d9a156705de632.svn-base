		<script type="text/javascript">
            function levelIcons(level){
                var isun = "<em class='level_sun'></em>";
                var imoon = "<em class='level_moon'></em>";
                var istar = "<em class='level_star'></em>";
                var icons = "";
                var sunNum = Math.floor(level/16);
                if(sunNum > 0){for(i=0;i<sunNum;i++){icons += isun;}}
                var moonNum = Math.floor((level-sunNum*16)/4);
                if(moonNum > 0){for(i=0;i<moonNum;i++){icons += imoon;}}
                var starNum = level-sunNum*16-moonNum*4;
                if(starNum > 0){for(i=0;i<starNum;i++){icons += istar;}}
                document.write(icons);
            }
		</script>
